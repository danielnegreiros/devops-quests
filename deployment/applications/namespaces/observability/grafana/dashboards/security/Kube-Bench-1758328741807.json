          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  tr.scan_timestamp AS \"Time\",\r\n  r.cluster_name AS \"Cluster\",\r\n  tr.test_number AS \"Test #\",\r\n  tr.test_description AS \"Description\",\r\n  tr.status AS \"Status\",\r\n  tr.scored AS \"Scored\",\r\n  tr.remediation AS \"Remediation\"\r\nFROM test_results tr\r\nJOIN kube_bench_reports r ON r.id = tr.report_id\r\nJOIN (\r\n    SELECT MAX(scan_timestamp) AS last_scan\r\n    FROM kube_bench_reports\r\n) latest ON r.scan_timestamp = latest.last_scan\r\nORDER BY tr.test_number;\r\n",
